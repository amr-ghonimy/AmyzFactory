                    <a class="btn hvr-hover" href="#">برامج التغذية</a>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Categories -->
 <!-- Start About Page  -->
@{
    Html.RenderAction("_GetInfo", "HomePreview");
}
<!-- End About Page -->
<!-- Start Products  -->
<div class="products-box">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="title-all text-center">
                    <h1>أقسامنا</h1>
                    <p>أقسام أميز للعلف</p>
                </div>
            </div>
        </div>
        @{
            Html.RenderAction("_GetDepartments", "HomePreview");
        }

    </div>
</div>
<!-- End Products  -->
